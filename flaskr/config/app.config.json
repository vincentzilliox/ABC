{
	"Merge TSV":{
		"app":"IRC",
		"script":"python2.7",
		"script_location":"/app/flaskr/toolkit/tsvToCanDiD.py",
		"output":"stdout",
		"output_file_extension":"merged.final.tsv",
		"comment":"Merge multiple STARK tsv files in one",
		"options":[
			{
				"option":"-a",
				"comment":"list of input file (SAMPLE.final.tsv)",
				"name":"input file",
				"type":"file",
				"sep":","
			},
			{
				"option":"-c",
				"type":"file",
				"default":"/app/flaskr/config/configuration.concat.json"
			},
			{
				"option":"--stdout",
				"type":"boolean"
			}
		]
	},
	"Cartagenia":{
		"app":"DIAG",
		"script":"sh",
		"script_location":"/app/flaskr/toolkit/cartagenia.sh",
		"output":"-o",
		"output_file_extension":"cartagenia.reclustering.txt",
		"comment":"Create a Cartagenia merged file",
		"options":[
			{
				"option":"-f",
				"comment":"fichier Catagenia Femme",
				"name":"Femmes",
				"type":"file"
			},
			{
				"option":"-m",
				"comment":"fichier Catagenia Homme",
				"name":"Hommes",
				"type":"file"
			},
			{
				"option":"-r",
				"comment":"fichier Catagenia Reclustering",
				"name":"Reclustering",
				"type":"file"
			}
		]
	},
	"Args":{
		"app":"DIAG",
		"script":"sh",
		"script_location":"/app/flaskr/toolkit/args_test.sh",
		"output":"stdout",
		"output_file_extension":"test_args.txt",
		"comment":"This page test a command that contain args",
		"options":[
			{
				"name":"File",
				"comment":"choose a file",
				"type":"file"
			},
			{
				"name":"Argument",
				"comment":"choose a argument",
				"choice":["a","b","c","d","e"],
				"default":"b",
				"type":"text"
			}
		]
	}

}



















